touch BMW Audi Mercedes
mkdir car_makers
touch Yamaha Honda Kawasaki
mkdir moto_makers
mv BMW car_makers/ && mv Audi car_makers/ && mv Mercedes car_makers/
mv Yamaha moto_makers/ && mv Kawasaki moto_makers/
mv Honda car_makers/moto_makers/

cls car_makers
cls moto_makers
cls car_makers/moto_makers
cls moto_makers/car_makers
cls @

cp ../README.md .
mkdir project
cp ../CMakeLists.txt ./project/CMakeLists.txt && mv README.md project/

rm BMW
rm Kawasaki
